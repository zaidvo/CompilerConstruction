# Test array operations
array numbers = [1, 2, 3, 4, 5]

# Manual sum
int total = 0
int i = 0
repeat 5 times:
    total = total + numbers[i]
    i = i + 1
end
print "sum([1,2,3,4,5]):"
print total

# Manual max
int maximum = numbers[0]
int j = 1
repeat 4 times:
    if numbers[j] > maximum:
        maximum = numbers[j]
    end
    j = j + 1
end
print "max:"
print maximum

# Manual min
int minimum = numbers[0]
int k = 1
repeat 4 times:
    if numbers[k] < minimum:
        minimum = numbers[k]
    end
    k = k + 1
end
print "min:"
print minimum

print "All manual calculations completed!"

